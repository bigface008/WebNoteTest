<hibernate-mapping package="webnote">
    <class name="ProblemEntity" table="problem">
    <id name="problemId" column="problemID">
        <generator class="native"/>
    </id>
    <property name="userName"/>
    <property name="problemName"/>
    <property name="description"/>
    <property name="reason"/>
    <property name="subject"/>
    <property name="semester"/>
    <property name="addDate"/>
    <property name="latestEditDate"/>
    <property name="redoTimes"/>
    <property name="answer"/>
    <property name="tags"/>
    </class>
</hibernate-mapping>
